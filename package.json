{
   "name": "expres",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "build-app": "docker-compose build ",
      "stop-app": "docker-compose down",
      "start-app": "docker-compose up -d && docker-compose logs app -f",
      "dev": "nodemon",
      "prettier": "prettier --write '{src,test,scripts}/**/*.ts' && prettier --write '**/*.{yml,yaml}'",
      "fix-lint": "eslint '*/**/*.{ts,tsx}' --quiet --fix && npm run prettier",
      "stop-prod": "sudo docker-compose -f docker-compose-prod.yml down",
      "start-prod": "npm run stop-prod && sudo docker-compose -f docker-compose-prod.yml up -d",
      "build-prod": "npx tsc && sudo docker-compose -f docker-compose-prod.yml build",
      "prod": "APP_PORT=443 node dist/App.js",
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "reflect-metadata": "^0.1.3"
   },
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/node": "^16.11.10",
      "@typescript-eslint/eslint-plugin": "^6.2.0",
      "@typescript-eslint/parser": "^6.2.0",
      "eslint": "^8.45.0",
      "nodemon": "^3.0.1",
      "prettier": "^3.0.0",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
