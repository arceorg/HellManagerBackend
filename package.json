      "@types/passport-jwt": "^3.0.10",
      "@types/passport-local": "^1.0.35",
      "@types/pg": "^8.10.2",
      "@types/uuid": "^9.0.4",
      "bcrypt": "^5.1.1",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.2",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "passport-local": "^1.0.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "ts-node": "^10.9.1",
      "typeorm": "0.3.17",
      "uuid": "^9.0.1"
   }
